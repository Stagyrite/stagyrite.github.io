get = {
    case [], _ -> nil
    case [x, *y], 0 -> x
    case [x, *y], n -> get(y, n - 1)
}

getName = (x, language) -> {

        if (language == "en") {
                index = 0
        } else if (language == "pl") {
                index = 1
        } else if (language == "fr") {
                index = 2
        } else if (language == "de") {
                index = 3
        } else {
                ""
        }

        get(x, index)
}

makeProduct = (row) -> {
    name = getName(row, "en")
    inPolish = getName(row, "pl")
    inFrench = getName(row, "fr")
    inGerman = getName(row, "de")
    product = kvs()
    product.put("name", name)
    product.put("name_pl", inPolish)
    product.put("name_fr", inFrench)
    product.put("name_de", inGerman)
    product
}

